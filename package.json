{
  "name": "deadminecraft",
  "version": "1.0.0",
  "description": "500 bot chaos experiment - Mass Minecraft bot controller with dashboard",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node dist/server/index.js",
    "dev": "tsx watch src/server/index.ts",
    "build": "tsc",
    "clean": "rm -rf dist",
    "dashboard": "cd dashboard && bun run dev",
    "dashboard:build": "cd dashboard && bun run build",
    "setup": "bun install && cd dashboard && bun install",
    "legacy": "bun farmer-bot.js"
  },
  "dependencies": {
    "canvas": "^3.2.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mineflayer": "^4.20.1",
    "mineflayer-auto-eat": "^5.0.3",
    "mineflayer-collectblock": "^1.6.0",
    "mineflayer-pathfinder": "^2.4.4",
    "mineflayer-pvp": "^1.3.2",
    "prismarine-viewer": "^1.33.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/ws": "^8.5.10",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
